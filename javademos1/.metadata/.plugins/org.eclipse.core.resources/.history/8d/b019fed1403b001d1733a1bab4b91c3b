
public class Main {

	public static void main(String[] args) {
		Product product = new Product();
		product.name = "Laptop";
		product.id = 11;
		product.description = "Monster Laptop";
		product.price = 5300;
		product.stockAmount = 10;

		ProductManager productManager = new ProductManager();
		productManager.Add(product);

	}

}
